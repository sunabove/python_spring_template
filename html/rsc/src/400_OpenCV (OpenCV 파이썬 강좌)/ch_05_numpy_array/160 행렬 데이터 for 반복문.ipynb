    "import numpy as np\n",
    "\n",
    "def f(y, x):\n",
    "   return 4*y + x + 1\n",
    "\n",
    "a = np.fromfunction(f, (3, 4), dtype=int)\n",
    "\n",
    "print( \"a = \", a, sep=\"\\n\")\n",
    "print( \"-\"*30 )\n",
    "\n",
    "for i, row in enumerate(a):\n",
    "   print( \"row \", i + 1, \" =\", row )\n",
    "\n",
    "print( \"-\"*30 )\n",
    "for element in a.flat :\n",
    "    print( element, end=\",\")\n",
    " "
   ]
  }
