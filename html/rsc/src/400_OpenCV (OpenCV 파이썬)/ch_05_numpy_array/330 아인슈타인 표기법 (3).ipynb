   "source": [
    "# 행렬 대각합\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "# 행렬\n",
    "a = np.array([[1, 2, 3], [3, 4, 5], [4, 5, 6]])\n",
    "\n",
    "# 아인슈타인 표기법을 사용한 대각합 계산\n",
    "trace = np.einsum( 'ii->', a )\n",
    "# 'ii->'는 동일한 인덱스의 대각 요소들을 합산하라는 의미입니다.\n",
    "\n",
    "print( \"a = \", a, sep=\"\\n\" )\n",
    "print(\"Trace of the matrix:\", trace)\n",
    "# Output: 5 (1 + 4)\n"
   ]
  }
