    "# Numpy 배열 복사\n",
    "import numpy as np\n",
    "\n",
    "a = np.array( [ 1, 2, 3] )\n",
    "b = a  # 포인터\n",
    "c = a[:] # 복사\n",
    "d = c.copy() # 복사\n",
    "\n",
    "# 배열 값 출력\n",
    "print( \"a =\", hex(id(a)), \",\", a )\n",
    "print( \"b =\", hex(id(b)), \",\", b ) # 포인터\n",
    "print( \"c =\", hex(id(c)), \",\", c ) # 복사\n",
    "print( \"d =\", hex(id(d)), \",\", d ) # 복사"
   ]
